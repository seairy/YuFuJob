<div class="row">
  <div class="col-xs-1"></div>
  <div class="col-xs-4 job-list">
    <ul>
      <% @left_jobs.each do |job| %>
      <li>
        <div class="job-list-content">
          <span class="title"><%= link_to job.title, job %></span>
          <span class="started-at pull-right"><%= job.started_at %></span>
        </div>
      </li>
      <% end if @left_jobs %>
    </ul>
  </div>
  <div class="col-xs-2"></div>
  <div class="col-xs-4 job-list">
    <ul>
      <% @right_jobs.each do |job| %>
      <li>
        <div class="job-list-content">
          <span class="title"><%= link_to job.title, job %></span>
          <span class="started-at pull-right"><%= job.started_at %></span>
        </div>
      </li>
      <% end if @right_jobs %>
    </ul>
  </div>
  <div class="col-xs-1"></div>
  <div class="col-xs-12 text-center">
    <%= paginate @jobs %>
  </div>
</div>
